{
  "name": "perfilov/php-dto-benchmark",
  "description": "Benchmark suite for comparing PHP DTO, serializer, hydrator and mapper libraries.",
  "type": "project",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "license": "MIT",
  "keywords": [
    "php",
    "dto",
    "data transfer object",
    "serializer",
    "hydrator",
    "mapper",
    "packer",
    "benchmark",
    "performance",
    "profiling",
    "testing",
    "comparison"
  ],
  "version": "1.1.0",
  "time": "2025-09-09",
  "authors": [
    {
      "name": "Perfilov Stanislav",
      "email": "stanislav.perfilov@gmail.com",
      "homepage": "https://beacons.ai/stanislavperfilov",
      "role": "developer"
    }
  ],
  "autoload": {
    "psr-4": {
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "phpbench/phpbench": "1.2.8",
    "yzen.dev/plain-to-class": "^3.0",
    "eventsauce/object-hydrator": "^1.6",
    "jane-php/automapper": "^7.5",
    "phpdocumentor/reflection-docblock": "^5.3",
    "cuyz/valinor": "^2.1",
    "spatie/data-transfer-object": "^3.9",
    "symfony/property-access": "^6.4",
    "symfony/serializer": "^6.3",
    "perfilov/php-dto-packer": "^5.1",
    "dragon-code/simple-dto": "^2.8"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.28",
    "symfony/var-dumper": "^6.2"
  },
  "scripts": {
    "test": "php ./vendor/bin/phpbench run tests/Benchmark/FullCheckBench.php --report='{\"extends\": \"aggregate\", \"cols\": [\"subject\", \"mem_peak\", \"best\", \"worst\", \"mean\", \"mode\"]}'\n"
  }
}
